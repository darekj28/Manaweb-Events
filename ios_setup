/usr/bin/ruby -e "$(curl -fsSL https://raw.githubusercontent.com/Homebrew/install/master/install)"
brew install node
brew install watchman
sudo npm install -g react-native-cli
cd mobile/feed
npm install --save rc-menu react react-dom react-native react-native-dismiss-keyboard react-native-gifted-spinner react-native-modal-dropdown react-native-modal-picker react-native-router-flux react-native-vector-icons react-native-datepicker 
npm install —-save react-native-facebook-login
npm install —-save react-native-fbsdk
npm install react-native-modal-picker --save
npm install --save react-native-loading-spinner-overlay@latest
npm install --save react-native-icon-badge

react-native run-ios